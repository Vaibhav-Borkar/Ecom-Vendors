spring.application.name=E-Commerce-Multi-Vendors

# Database Configuration
spring.datasource.url=jdbc:mysql://localhost:3306/ecom_vendors
spring.datasource.username=root
spring.datasource.password=root
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver

# JPA Configuration
spring.jpa.hibernate.ddl-auto=update  # Possible values: none, validate, update, create, create-drop
spring.jpa.show-sql=true        
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQL8Dialect
